# mkAdv: Prep -> NP -> Adv
# (:time (var (prep (:op1 (var (date-entity (:month [1-12]))))))) => (mkAdv prep_Prep (mkNP (monthPN [1-12]_Month)))
/:time/=time < (/VAR/ < (/PREP/=prep <: (/:op1/=op < (/VAR/ < (/date-entity/ <: (/:month/ < /NUM/=month))))))
[relabel prep /^(.+)$/$1_Prep/]
