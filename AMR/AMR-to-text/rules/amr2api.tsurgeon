# mkAdv : Prep -> NP -> Adv
# (var (prep NP)) => (mkAdv prep_Prep NP)
/VAR/=var < (/PREP/=prep < /NP/=np)
[relabel prep /^(.+)$/$1_Prep/]
[adjoin (mkAdv=adv TEMP=temp@) var]
[move np >2 adv]
[excise temp temp]

# mkAdv : Prep -> NP -> Adv
# (var (prep (:op1 NP))) => (mkAdv prep_Prep NP)
/VAR/=var < (/PREP/=prep < (/:opN/=op < /NP/=np))
[relabel prep /^(.+)$/$1_Prep/]
[adjoin (mkAdv=adv TEMP=temp@) var]
[move np >2 adv]
[excise temp temp]
[delete op]

# mkVP : VP -> Adv -> VP
# (VP (frame (:degree (var multi-word-expression)))) => (mkVP (VP frame) (mkAdv "multi word expression"))
/VP/=vp_i < (/FRAME/ < (/:degree/=degree < (/VAR/ < /^~/WORD/([-]~/WORD/)*$/=mwe)))
[relabel mwe /^(.+)$/"$1"/]
[relabel mwe /(-)/ /]
[adjoinF (P.mkAdv=adv @) mwe]
[adjoinF (mkVP=vp_o @) vp_i]
[move adv >2 vp_o]
[delete degree]

# mkVP : VP -> Adv -> VP
# (VP (frame (:ARG[2-9] NP))) => (mkVP (VP frame) (mkAdv prep_Prep NP))
/VP/=vp_i < (/FRAME/ < (/:ARGn/=prep < /NP/=np))
[relabel prep /^(.+)-(.+)$/$2_Prep/]
[adjoinF (mkAdv=adv @) prep]
[move np >2 adv]
[adjoinF (mkVP=vp_o @) vp_i]
[move adv >2 vp_o]

# mkVP : VP -> Adv -> VP
# (VP (frame (:ARG[0-9] mkAdv))) => (mkVP (VP frame) mkAdv)
/VP/=vp_i [[< /FRAME/ < (/:ARGx/=arg < mkAdv=adv)] | [< (/FRAME/ < (/:ARGx/=arg < mkAdv=adv))]]
[adjoinF (mkVP=vp_o @) vp_i]
[move adv >2 vp_o]
[delete arg]

# mkAP : AdA -> AP -> AP
# (mkAP (adj_A|frame (:degree|:mod (var ada)))) => (mkAP ada_AdA (mkAP adj_A|frame))
mkAP=ap_i < (/^(~/adj_A/|~/FRAME/)$/ < (/^:(degree|mod)$/=mod < (/VAR/ < /WORD/=ada)))
[adjoinF (mkAP=ap_o @) ap_i]
[move ada >1 ap_o]
[relabel ada /^(.+)$/L.$1_AdA/]
[delete mod]

# mkAP : AdA -> AP -> AP
# (mkAP (adjA|frame (:degree|:mod (var multi-word-expression)))) => (mkAP (mkAdA "mwe") (mkAP adjA|frame))
mkAP=ap_i < (/^(~/adj_A/|~/FRAME/)$/ < (/^:(degree|mod)$/=mod < (/VAR/ < /^~/WORD/([-]~/WORD/)*$/=mwe)))
[relabel mwe /^(.+)$/"$1"/]
[relabel mwe /(-)/ /]
[adjoinF (P.mkAdA=ada @) mwe]
[adjoinF (mkAP=ap_o @) ap_i]
[move ada >1 ap_o]
[delete mod]

# mkListS : S -> S -> ListS
# (conj (:op mkS) (:op mkS)) => (conj (mkListS mkS mkS))
/CONJ/ [[<1 (/:opN/=op1 < mkS=s1) <2 (/:opN/=op2 < mkS=s2)] | [<2 (/:opN/=op1 < mkS=s1) <3 (/:opN/=op2 < mkS=s2)]]
[adjoinF (mkListS=list @) s1]
[move s2 >2 list]
[excise op1 op1]
[delete op2]

# mkListS : S -> ListS -> ListS
# (conj mkListS (:op mkS)) => (conj (mkListS mkS mkListS))
/CONJ/ [[<1 mkListS=list_i <2 (/:opN/=op < mkS=s)] | [<2 mkListS=list_i <3 (/:opN/=op < mkS=s)]]
[adjoinF (mkListS=list_o @) s]
[move list_i >2 list_o]
[excise op op]

# mkS : Conj -> ListS -> S
# (var (conj mkListS)) => (mkS (conj_Conj mkListS))
/VAR/=var < (/CONJ/=conj < mkListS=list)
[adjoinF (mkS=s @) conj]
[relabel conj /^(.+)$/S.$1_Conj/]
[move list >2 s]
[excise var var]

# PurposeVP : VP -> Adv
# (passiveVP (frame mkVP)) => (mkVP (passiveVP frame) (PurposeVP mkVP))
/passiveVP/=pass < (/FRAME/=frame < mkVP=vp_i)
[adjoinF (mkVP=vp_o @) pass]
[adjoinF (E.PurposeVP=adv @) vp_i]
[move adv >2 vp_o]

# (VP (frame (:manner (var (noun (:mod (var adj))))))) => (mkVP (VP frame) (mkAdv in_Prep (mkNP a_Det (mkCN adj_A noun_N))))
/VP/=vp_i < (/FRAME/ < (/:manner/=manner < (/VAR/ < (/WORD/=noun <: (/:mod/=mod < (/VAR/ < /WORD/=adj))))))
[relabel noun /^(.+)$/$1_N/]
[relabel adj /^(.+)$/$1_A/]
[adjoinF (mkVP=vp_o @) vp_i]
[adjoinF (mkAdv=adv in_Prep (mkNP S.a_Det (mkCN=cn @))) noun]
[move adj >1 cn]
[delete mod]
[move adv >2 vp_o]
[delete manner]

# mkVP : AdV -> VP -> VP
# mkAdV : Str -> AdV
# (VP (frame (:mod|:manner (var AdV)))) => (mkVP AdV (VP frame))
/VP/=vp_i < (/FRAME/ < (/^(:mod|:manner|:quant)$/=mod <: (/VAR/ <: (/WORD/=adv1 !< /ANY/))))
[relabel adv1 /^~/ADJ_ADV/$/$1ly/]
[relabel adv1 /^(.+)llly$/$1lly/]
[relabel adv1 /^(.+)lely$/$1ly/]
[relabel adv1 /^(.+)vyly$/$1vily/]
[relabel adv1 /^(.+)$/"$1"/]
[adjoinF (mkAdV=adv2 @) adv1]
[adjoinF (mkVP=vp_o @) vp_i]
[move adv2 >1 vp_o]
[delete mod]

# mkVP : AdV -> VP -> VP
# mkAdV : Str -> AdV
# (VP (frame (:time (var AdV)))) => (mkVP AdV (VP frame))
/VP/=vp_i < (/FRAME/ < (/:time/=time <: (/VAR/ <: (/WORD/=adv1 !< /ANY/))))
[relabel adv1 /^~/ADJ_ADV/$/$1ly/]
[relabel adv1 /^(.+)$/"$1"/]
[adjoinF (mkAdV=adv2 @) adv1]
[adjoinF (mkVP=vp_o @) vp_i]
[move adv2 >1 vp_o]
[delete time]

# mkVP : VP -> Adv -> VP
# mkVP : AP -> VP
# (mkAP (adj_A|frame (mkAdv if_Subj [mkS]))) => (mkVP (mkVP mkAP) mkAdv)
mkAP=ap < (/^(~/adj_A/|~/FRAME/)$/ < (mkAdv=adv < /if_Subj/))
[adjoinF (mkVP=vp (mkVP @)) ap]
[move adv >2 vp]

# mkAP : AP -> S -> AP
# (mkAP (adj_A|frame (:ARG1 mkS))) => (mkAP (mkAP adj_A|frame) mkS)
mkAP=ap_i < (/^(~/adj_A/|~/FRAME/)$/ < (/:ARG1/=arg < mkS))
[adjoinF (mkAP=ap_o @) ap_i]
[move arg >2 ap_o]
[excise arg arg]

# mkCl : NP -> Cl
# mkS : Cl -> S
# mkAP : AP -> S -> AP
# (mkAP (adj_A|frame (:ARG1 NP))) => (mkAP (mkAP adj_A|frame) (mkS (mkCl NP)))
mkAP=ap_i < (/^(~/adj_A/|~/FRAME/)$/ < (/:ARG1/=arg < /NP/))
[adjoin (mkS=obj (mkCl@)) arg]
[adjoinF (mkAP=ap_o @) ap_i]
[move obj >2 ap_o]

# mkS : Pol -> Cl -> S
# (S (Cl (VP (frame (:polarity -))))) => (S negativePol (Cl ..))
/S/=snt < (/Cl/ <+(/VP/) (/FRAME/ < (/:polarity/=pol < -)))
[insert (negativePol) >1 snt]
[delete pol]

# mkS : Pol -> Cl -> S
# (S (Cl (NP (CN (frame (:polarity -)))))) => (S negativePol (Cl ..))
/S/=snt < (/Cl/ <+(/NP/) (/CN/ < (/FRAME/ < (/:polarity/=pol < -))))
[insert (negativePol) >1 snt]
[delete pol]

# mkS : Pol -> Cl -> S
# (S (Cl (mkAP (frame (:polarity -))))) => (S negativePol (Cl ..))
/S/=snt < (/Cl/ <+(mkAP) (/FRAME/ < (/:polarity/=pol < -)))
[insert (negativePol) >1 snt]
[delete pol]

# (mkVP (mkS (mkCl (mkVP (frame (:mode interrogative)))))) => (mkVP mkVP (mkAdv if_Subj mkS))
mkVP=vp_i < (mkS=s < (mkCl < (mkVP < (/FRAME/ < (/:mode/=mode < interrogative)))))
[delete mode]
[adjoinF (mkVP=vp_o @) vp_i]
[adjoinF (mkAdv=adv S.if_Subj @) s]
[move adv >2 vp_o]

# (mkAP mkAP (mkS (conj_Conj (:mode interrogative)))) => (mkVP (mkVP mkAP) (mkAdv if_Subj mkS))
mkAP=ap < mkAP < (mkS=s [[< (/conj_Conj/ < (/:mode/=mode < interrogative))] | [< (/:mode/=mode < interrogative)]])
[delete mode]
[relabel ap /^.+$/mkVP/]
[adjoinF (mkVP=vp @) ap]
[adjoinF (mkAdv=adv S.if_Subj @) s]
[move adv >2 vp]

# mkNP : Predet -> NP -> NP
# (mkNP (:mod (var word)) mkPN) => (mkNP (ss "word") (mkNP mkPN))
mkNP=np_i <1 (/:mod/=mod < (/VAR/ <: /WORD/=word)) <2 mkPN
[adjoinF (mkNP=np_o ss=predet @) np_i]
[relabel word /^(.+)$/"$1"/]
[move word >1 predet]
[delete mod]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : IAdv -> Cl -> QCl
# (ROOT (mkS (mkCl (VP frame (var amr-unknown))))) => (mkText (mkUtt (mkQS (mkQCl what_IAdv mkCl))) questMarkPunct)
mkS=s < (mkCl=cl <+(/VP/) (/FRAME/ $+ (/VAR/=wh < /amr-unknown/))) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl=qcl L.what_IAdv @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete wh]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : IAdv -> Cl -> QCl
# (ROOT (mkS (mkCl (VP (frame (:manner (var amr-unknown))))))) => (mkText (mkUtt (mkQS (mkQCl how_IAdv mkCl))) questMarkPunct)
mkS=s < (mkCl=cl < (/VP/ < (/FRAME/ < (/:manner/=wh < (/VAR/ < /amr-unknown/))))) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl=qcl S.how_IAdv @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete wh]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : IAdv -> Cl -> QCl
# (ROOT (mkS (mkCl (mkAP (mkS (mkCl (VP (frame (:manner (var amr-unknown)))))))))) => (mkText (mkUtt (mkQS (mkQCl how_IAdv mkCl))) questMarkPunct)
mkS=s < (mkCl=cl < (mkAP < (mkS < (mkCl < (/VP/ < (/FRAME/ < (/:manner/=wh < (/VAR/ < /amr-unknown/)))))))) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl=qcl S.how_IAdv @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete wh]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : IAdv -> Cl -> QCl
# (ROOT (mkS (mkCl (VP (frame (:location (var amr-unknown))))))) => (mkText (mkUtt (mkQS (mkQCl where_IAdv mkCl))) questMarkPunct)
mkS=s < (mkCl=cl < (/VP/ < (/FRAME/ < (/:location/=wh < (/VAR/ < /amr-unknown/))))) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl=qcl S.where_IAdv @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete wh]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : IAdv -> Cl -> QCl
# mkIAdv : IAdv -> Adv -> IAdv
# (ROOT (mkS (mkCl (VP (frame (:duration (var amr-unknown))))))) => (mkText (mkUtt (mkQS (mkQCl (mkIAdv how_IAdv (mkAdv long_A)) mkCl))) questMarkPunct)
mkS=s < (mkCl=cl < (/VP/ < (/FRAME/ < (/:duration/=wh < (/VAR/ < /amr-unknown/))))) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl=qcl (mkIAdv S.how_IAdv (mkAdv long_A)) @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete wh]

# mkText : Utt -> Punct -> Text
# mkUtt : QS -> Utt
# mkQS : QCl -> QS
# mkQCl : Cl -> QCl
# (ROOT (mkS (mkCl (.. (:mode interrogative))))) => (mkText (mkUtt mkQS) questMarkPunct)
mkS=s < (mkCl=cl << (/:mode/=mode < interrogative)) [> /:sntN/ | !> /ANY/]
[relabel s /^.+$/mkQS/]
[adjoinF (mkQCl @) cl]
[adjoinF (mkText (mkUtt @) questMarkPunct) s]
[delete mode]

# mkText : Utt -> Punct -> Text
# mkUtt : S -> Utt
# (ROOT (mkS (.. (:mode expressive|imperative)))) => (mkText (mkUtt mkS) exclMarkPunct)
mkS=s << (/:mode/=mode < /expressive|imperative/) [> /:sntN/ | !> /ANY/]
[adjoinF (mkText (mkUtt @) exclMarkPunct) s]
[delete mode]

# mkText : Utt -> Punct -> Text
# mkUtt : S -> Utt
# (ROOT mkS) => (mkText (mkUtt mkS) fullStopPunct)
mkS=s [> /:sntN/ | !> /ANY/]
[adjoinF (mkText (mkUtt @) fullStopPunct) s]

# mkText : Utt -> Text
# mkUtt : NP -> Utt
# (ROOT NP) => (mkText (mkUtt NP))
/NP/=np [> /:sntN/ | !> /ANY/]
[adjoinF (mkText (mkUtt @)) np]

# mkText : Text -> Text -> Text
# NB: To avoid cases where linearization of a whole paragraph fails because of a single failing sentence,
# the recursive mkText is actually not used (SemEval2017).
# ((:sntN mkText) (:sntN mkText) [..]) => (:sntN mkText mkText [..])
(/:sntN/=snt1 < mkText=txt1) $+ (/:sntN/=snt2 < mkText=txt2)
[move txt2 >-1 snt1]
[delete snt2]

# mkNP : Det -> CN -> NP
# (mkNP S.a_Quant (:quant (s2 several)) mkCN) => (mkNP L.several_Det mkCN)
mkNP <1 /a_Quant/=quant1 <2 (/:quant/=quant2 < (/VAR/=var < /DET_QUANT/=det)) <3 mkCN
[delete quant1]
[excise quant2 var]
[relabel det /^(.+)$/L.$1_Det/]

### Post-editing

# NB: To preserve a single-rooted tree, the multi-sentence construction is simplified but not fully excised.
# To be excised in an external post-processing step.
# (m (multi-sentence (:sntN mkText mkText [..]))) => (multi-sentence mkText mkText [..])
/VAR/=var < (/multi-sentence/ < (/:sntN/=snt < mkText))
[excise snt snt]
[excise var var]

# (mkS (and_Conj mkAdv) (mkListS mkS (mkS (mkCl VP)))) => (mkS and_Conj (mkListS mkS (mkS (mkCl (mkVP VP mkAdv)))))
mkS < (/conj_Conj/ < mkAdv=adv) < (mkListS <- (mkS < (mkCl < /VP/=vp_i)))
[adjoinF (mkVP=vp_o @) vp_i]
[move adv >2 vp_o]

# (mkNP a_Quant (mkCN definite_N)) => (mkNP the_Quant mkCN)
mkNP < /a_Quant/=quant <+(mkCN) /definite_N/
[relabel quant /a_Quant/the_Quant/]

# (mkNP a_Quant (CompoundCN noun_N definite_N)) => (mkNP the_Quant CompoundCN)
mkNP <1 /a_Quant/=quant <2 (/CompoundCN/ <1 /noun_N/ <2 /definite_N/)
[relabel quant /a_Quant/the_Quant/]

# (mkNP a_Quant (mkCN uncountable_N)) => (mkNP mkCN)
mkNP < /a_Quant/=quant <+(mkCN) /uncountable_N/
[delete quant]

# (mkNP a_Quant (CompoundCN noun_N uncountable_N)) => (mkNP CompoundCN)
mkNP <1 /a_Quant/=quant <2 (/CompoundCN/ <1 /noun_N/ <2 /uncountable_N/)
[delete quant]

# (mkNP a_Quant (CN other_A noun_N)) => (mkNP aPl_Det (CN other_A noun_N))
mkNP < /a_Quant/=quant !< mkNum <+(/CN/) (other_A $+ /noun_N/)
[relabel quant /a_Quant/aPl_Det/]

# (mkAP frame) => (mkAP frame_A)
mkAP < /FRAME/=frame
[relabel frame /^(.+)-(.+)$/$1_A/]

# (mkCl mkAP) => (mkCl S.it_NP mkAP)
mkCl=cl <: mkAP
[insert (S.it_NP) >1 cl]

# (mkCN mkCN (mkRS (mkRCl which_RP (mkAP adj_A)))) => (mkCN adj_A (mkCN noun_N))
mkCN=cn < mkCN < (mkRS=rs !< negativePol < (mkRCl < /which_RP/ < (mkAP <: /adj_A/=adj)))
[move adj >1 cn]
[delete rs]

# (mkAdv because_Subj (:ARG0 mkS)) => (mkAdv because_Subj mkS)
mkAdv <1 /because_Subj/ <2 (/:ARG0/=arg < mkS)
[excise arg arg]

# (mkAdv because_Subj (:ARG0 mkNP)) => (mkAdv because_Subj (mkS (mkCl mkNP)))
mkAdv <1 /because_Subj/ <2 (/:ARG0/=arg < mkNP)
[adjoin (mkS (mkCl@)) arg]

# (VP (frame Adv)) => (mkVP (VP frame) Adv)
/VP/=vp_i < (/FRAME/ < /Adv/=adv)
[adjoinF (mkVP=vp_o @) vp_i]
[move adv >2 vp_o]

# (VP frame var) => (VP frame)
/VP/ <1 /FRAME/ <2 (/VAR/=var !< /ANY/)
[delete var]

# (CN (noun_N Adv)) => (mkCN (CN noun_N) Adv)
/CN/=cn_i < (/noun_N/ < /Adv/=adv)
[adjoinF (mkCN=cn_o @) cn_i]
[move adv >2 cn_o]

# (mkCl CN) => (mkCl (mkNP CN))
mkCl < /CN/=cn
[adjoinF (mkNP @) cn]

# (:ARG[01] NP) => (NP)
/:ARGm/=arg < /NP/
[excise arg arg]

# (mkVP VP) => (mkVP)
mkVP=vp_o <: /VP/
[excise vp_o vp_o]

# (prep_Prep (:op1 mkNP)) => (prep_Prep mkNP)
/prep_Prep/=prep <: (/:op1/=op < mkNP=np)
[move np $- prep]
[delete op]

# (passiveVP frame) => (passiveVP frame_V2)
/passiveVP/ <: /FRAME/=frame
[relabel frame /^(.+)-(.+)$/L.$1_V2/]

# (mkVP frame NP) => (mkVP frame_V2 NP)
mkVP <1 /FRAME/=frame <2 /NP/
[relabel frame /^(.+)-(.+)$/L.$1_V2/]

# (mkRCl which_RP NP frame) => (mkRCl which_RP NP frame_V2)
mkRCl <1 /which_RP/ <2 /NP/ <3 /FRAME/=frame
[relabel frame /^(.+)-(.+)$/L.$1_V2/]

# (mkVP frame mkVP) => (mkVP frame_VV mkVP)
mkVP <1 /FRAME/=frame <2 mkVP
[relabel frame /^(.+)-(.+)$/L.$1_VV/]

# (mkVP frame mkVP entity_NP) => (mkVP frame entity_NP mkVP)
mkVP=vp <1 /FRAME/=frame <2 mkVP <3 /struct_NP/=np
[relabel frame /^(.+)-(.+)$/L.$1_V2V/]
[move np >2 vp]

# (mkVP frame mkS) => (mkVP frame_VS mkS)
mkVP <1 /FRAME/=frame <2 mkS
[relabel frame /^(.+)-(.+)$/L.$1_VS/]

# (mkVP frame) => (mkVP frame_V)
mkVP <: /FRAME/=frame
[relabel frame /^(.+)-(.+)$/L.$1_V/]

# (CN frame) => (mkCN frame_N)
/CN/ < /FRAME/=frame
[relabel frame /^(.+)-(.+)$/L.$1_N/]

# mkN2 => P.mkN2
mkN2=n2
[relabel n2 /^(.+)$/P.$1/]

# mkPN => P.mkPN
mkPN=pn
[relabel pn /^(.+)$/P.$1/]

# mkAdv => P.mkAdv
mkAdv=adv < /MWE/
[relabel adv /^(.+)$/P.$1/]

# mkAdV => P.mkAdV
mkAdV=adv
[relabel adv /^(.+)$/P.$1/]

# mkAdv => S.mkAdv
mkAdv=adv
[relabel adv /^(.+)$/S.$1/]

# mkOrd => S.mkOrd
mkOrd=ord
[relabel ord /^(.+)$/S.$1/]

# prep_Prep => S.prep_Prep
possess_Prep=prep
[relabel prep /^(.+)$/S.$1/]

# prep_Prep => L.prep_Prep
/prep_Prep/=prep
[relabel prep /^(.+)$/L.$1/]

# noun_N => L.noun_N
/noun_N/=n
[relabel n /^(.+)$/L.$1/]

# adj_A => L.adj_A
/adj_A/=a
[relabel a /^(.+)$/L.$1/]

### Remove unresolved coreferences

/Cl/ <1 /VAR/=var
[delete var]

/:ARGx/=arg <: /VAR/
[delete arg]

/:location/=loc <: /VAR/
[delete loc]

/:poss/=poss <: /VAR/
[delete poss]

/:li/=li <: /NUM/
[delete li]

--- Remove unresolved relations, reifications, etc.

/VP/ <1 /FRAME/=frame <2 /VAR/=var
[delete var]
[relabel frame /^(.+)-(.+)$/L.$1_V/]

/REL/=unk
[delete unk]

/ROLE/=role
[adjoinF (mkNP S.a_Det L.person_N @) role]
[delete role]

#/REIF/=unk
#[delete unk]
