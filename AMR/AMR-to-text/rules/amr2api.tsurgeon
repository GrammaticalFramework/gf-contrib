### Pre-editing

# (verb-particle-id) => (verb_particle-id)
/VAR/ < /^[a-z]+-[a-z]+-[0-9]+$/=frame
[relabel frame /^(.+)-(.+)-(.+)$/$1_$2-$3/]

# ((:ARG0-of (var role)) :ord) => (:ARG0-of (var (role :ord)))
/:ARG0-of/ < (/VAR/ < /ROLE/=org) $ /:ord/=ord
[move ord >-1 org]

# (var ('person' :relation (:ARG0-of (var role)))) => (var role)
/VAR/ < (/person/ < /^:(name|location)$/=rel < (/:ARG0-of/ < (/VAR/ < /ROLE/=role)))
[move rel >-1 role]

# (var ('person' :ARG0-of (:ARG0-of (var role)))) => (var role)
/VAR/ < (/person/ < (/:ARG0-of/=arg !<< /ROLE/) < (/:ARG0-of/ < (/VAR/ < /ROLE/=role)))
[move arg >-1 role]

# (var ('person' (:ARG0-of (var role)))) => (var role)
/VAR/=var1 < (/person/ < (/:ARG0-of/ < (/VAR/=var2 < /ROLE/)))
[excise var1 var2]

# TODO: move other pre-editing rules here

### Rules

# mkNum : Digits -> Num
# mkDigits : Str -> Digits
# (:quant num) => (mkNum (mkDigits "num"))
/:quant/=quant < /NUM/=num
[relabel num /^(.+)$/"$1"/]
[adjoin (mkNum (mkDigits@)) quant]

# mkOrd : Digits -> Ord
# mkDigits : Str -> Digits
# (:ord (var ('ordinal-entity' (:value num)))) => (mkOrd (mkDigits "num"))
/:ord/=ord < (/VAR/ < (/^ordinal-entity$/ < (/:value/=value < /NUM/=num)))
[relabel num /^(.+)$/"$1"/]
[adjoinF (mkOrd (mkDigits @)) num]
[excise ord value]

# mkCN : N -> CN
# (:unit (var unit)) => (:unit (mkCN unit_N))
/:unit/ < (/VAR/=var < (/WORD/=unit !< /ANY/))
[relabel unit /^(.+)$/$1_N/]
[adjoinF (mkCN @) unit]
[excise var var]

# PurposeVP : VP -> Adv
# (:purpose (var frame)) => (E.PurposeVP (mkVP frame))
/:purpose/=adv < (/VAR/=var < /FRAME/)
[relabel adv /^:purpose$/E.PurposeVP/]
[relabel var /^.+$/mkVP/]

# mkNP : Quant -> Num -> CN -> NP
# (var (monetary-quantity mkNum (:unit [CN]))) => (mkNP a_Quant mkNum [CN])
/VAR/=var < (/monetary-quantity/=money < mkNum=num < /:unit/=unit)
[adjoinF (mkNP=np S.a_Quant @) num]
[move unit >3 np]
[excise unit unit]
[excise var money]

# mkN2 : N -> Prep -> N2
# (role (:ARG2 (var entity))) => (role (mkN2 entity_N of_Prep))
/ROLE/ < (/:ARG2/=title < (/VAR/=var < /WORD/=entity))
[relabel entity /^(.+)$/$1_N/]
[adjoinF (mkN2=n2 @) entity]
[insert (of_Prep) >2 n2]
[excise title var]

# mkCN : N2 -> NP -> CN
# (role :ARG1 mkN2) => (mkCN (mkN2 :ARG1))
/ROLE/=cn < (/:ARG1/=org $ mkN2=title)
[move title $+ org]
[adjoin (mkCN TEMP=temp@) cn]
[excise temp temp]

# mkCN : N2 -> CN
# (role mkN2) => (mkCN mkN2)
/ROLE/=role <: mkN2=n2
[adjoinF (mkCN @) n2]
[excise role role]

# mkRCl : RP -> NP -> RCl
# mkRS : RCl -> RS
# (:mod role) => (mkRS (mkRCl which_RP mkNP))
/:mod/=mod < /ROLE/=org
[adjoin (mkRS (mkRCl S.which_RP (mkNP S.a_Quant mkCN@))) org]

# mkRCl : RP -> NP -> RCl
# (:ARG[01]-of (var (frame_N))) => (mkRS (mkRCl which_RP mkNP))
/:ARGm-of/=inv < (/VAR/=var < /NOUN_FRAME/) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP (mkNP S.a_Quant mkCN@))) var]
[excise inv inv]

# mkRCl : RP -> AP -> RCl
# (:ARG[01]-of (var (frame_A))) => (mkRS (mkRCl which_RP mkAP))
/:ARGm-of/=inv < (/VAR/=var < /ADJ_FRAME/) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP mkAP@)) var]
[excise inv inv]

# mkRCl : RP -> VP -> RCl
# mkRS : RCl -> RS
# (:ARG0-of (var frame)) => (mkRS (mkRCl which_RP mkVP))
/:ARG0-of/=inv < (/VAR/=var < /FRAME/) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP mkVP@)) var]
[excise inv inv]

# mkAdv : Subj -> S -> Adv
# (:ARG1-of (var (cause-01 :ARG0))) => (mkAdv because_Subj mkS)
/:ARG1-of/=arg < (/VAR/ < (/cause-01/=cause < /:ARG0/=sub))
[adjoin (mkAdv S.because_Subj TEMP=temp@) sub]
[excise arg cause]
[excise temp temp]

# mkS : Adv -> S -> S
# mkAdv : Subj -> S -> Adv
# (var (cause-01 :ARG0 :ARG1)) => (mkS (mkAdv because_Subj mkS) mkS)
/VAR/=var < (/cause-01/=cause < /:ARG0/=sub1 < /:ARG1/=sub2)
[adjoinF (mkS=s (mkAdv S.because_Subj @)) sub1]
[move sub2 >2 s]
[excise sub2 sub2]
[excise var cause]

# mkRCl : RP -> NP -> V2 -> RCl
# mkRS : RCl -> RS
# (:ARG1-of (var (frame :ARG0))) => (mkRS (mkRCl which_RP :ARG0 frame))
/:ARG1-of/=inv < (/VAR/=var < (/FRAME/=frame < /:ARG0/=subj)) !> /FRAME/
[adjoinF (mkRS (mkRCl=cl S.which_RP @)) frame]
[move subj >2 cl]
[excise inv var]

# passiveVP : V2 -> VP
# mkRCl : RP -> VP -> RCl
# mkRS : RCl -> RS
# (:ARG1-of (var frame)) => (mkRS (mkRCl which_RP (passiveVP frame)))
/:ARG1-of/=inv < (/VAR/=var < /FRAME/) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP (passiveVP@))) var]
[excise inv inv]

# mkListRS : RS -> RS -> ListRS
# (conj (:op mkRS) (:op mkRS)) => (conj (mkListRS mkRS mkRS))
/CONJ/ <1 (/:opN/=op1 < mkRS=rs1) <2 (/:opN/=op2 < mkRS=rs2)
[adjoinF (mkListRS=list @) rs1]
[move rs2 >2 list]
[excise op1 op1]
[delete op2]

# mkListRS : RS -> ListRS -> ListRS
# (conj mkListRS (:op mkRS)) => (conj (mkListRS mkRS mkListRS))
/CONJ/ <1 mkListRS=list_i <2 (/:opN/=op < mkRS=rs)
[adjoinF (mkListRS=list_o @) rs]
[move list_i >2 list_o]
[excise op op]

# mkRS : Conj -> ListRS -> RS
# (var (conj mkListRS)) => (mkRS (conj_Conj mkListRS))
/VAR/=var < (/CONJ/=conj < mkListRS=list)
[adjoinF (mkRS=rs @) conj]
[relabel conj /^(.+)$/S.$1_Conj/]
[move list >2 rs]
[excise var var]

# mkRCl : RP -> AP -> RCl
# mkAP : A -> AP
# (:ARG2-of (var (frame_A))) => (mkRS (mkRCl which_RP (mkAP frame_A)))
/:ARG2-of/=inv < (/VAR/=var [< /ADJ_FRAME/ | < /MAYBE_ADJ_FRAME/]) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP (mkAP@))) var]
[excise inv inv]

# mkRCl : RP -> NP -> RCl
# mkCN : N -> CN
# mkNP : Quant -> CN -> NP
# (:ARG2-of (var (frame_N))) => (mkRS (mkRCl which_RP mkNP))
/:ARG2-of/=inv < (/VAR/=var < /NOUN_FRAME/) !> /FRAME/
[adjoin (mkRS (mkRCl S.which_RP (mkNP S.a_Quant mkCN@))) var]
[excise inv inv]

# (mkCl !:ARG0 (mkVP (frame :ARG1 !:ARG0))) => (mkCl :ARG1 (mkVP (passiveVP frame)))
mkCl=cl !< /:ARG0/ < (mkVP < (/FRAME/=frame < /:ARG1/=subj !< /:ARG0/))
[relabel subj /^:ARG1$/:ARG0/]
[move subj >1 cl]
[excise subj subj]
[adjoinF (passiveVP @) frame]

# mkVP : VV -> VP -> VP
# (frame_VV (:ARG1 (var frame))) => (frame_VV (mkVP frame))
/VV_FRAME/ < (/:ARG1/=vp < (/VAR/=var < /FRAME/=vv))
[move vv >1 vp]
[relabel vp /^.+$/mkVP/]
[delete var]

# mkVP : VV -> VP -> VP
# (frame1 (:ARG2 (var (frame2 :ARG1)))) => (frame1 (mkVP frame2 :ARG1))
/FRAME/ < (/:ARG2/=vp < (/VAR/=var < (/FRAME/=vv < /:ARG1/=compl)))
[move vv >1 vp]
[move compl >2 vp]
[relabel vp /^.+$/mkVP/]
[delete var]

# mkVP : VV -> VP -> VP
# (frame1 (:ARG2 (var (frame2 (:ARG0 (var 'between')))))) => (frame1 (mkVP frame2 :ARG0))
/FRAME/ < (/:ARG2/=vp < (/VAR/=var < (/FRAME/=vv < (/:ARG0/=compl < (/VAR/ < between)))))
[move vv >1 vp]
[move compl >2 vp]
[relabel vp /^.+$/mkVP/]
[delete var]

# mkCN : N -> CN
# (:ARG[01] (var frame_N)) => (:ARG[01] (mkCN frame_N))
/:ARGm/ < (/VAR/=var < (/^(~/NOUN_FRAME/|~/MAYBE_NOUN_FRAME/)$/=noun !< /ANY/))
[adjoinF (mkNP S.a_Quant (mkCN @)) noun]
[excise var var]

# mkCl : NP -> Cl
# mkS : Cl -> S
# (var frame_N) => (mkS (mkCl mkNP))
/VAR/=var < /NOUN_FRAME/
[adjoin (mkS (mkCl (mkNP S.a_Quant mkCN@))) var]

# mkCl : AP -> Cl
# mkS : Cl -> S
# (var frame_A) => (mkS (mkCl mkAP))
/VAR/=var < /ADJ_FRAME/
[adjoin (mkS (mkCl mkAP@)) var]

# mkCl : VP -> Cl
# mkS : Cl -> S
# (var frame) => (mkS (mkCl mkVP))
/VAR/=var < /FRAME/ !> /:mod/
[adjoin (mkS (mkCl mkVP@)) var]

# mkNP : CN -> NP
# (frame (:ARG[01] CN)) => (frame (:ARG[01] (mkNP CN)))
/FRAME/ < (/:ARGm/ < /CN/=cn)
[adjoinF (mkNP @) cn]

# mkCl : NP -> VP -> Cl
