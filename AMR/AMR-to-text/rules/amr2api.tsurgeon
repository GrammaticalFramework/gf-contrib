# dayMonthYearAdv : Monthday -> Month -> Year -> Adv
# (:time (var (date-entity (:year year) (:month month) (:day day)))) => (dayMonthYearAdv (intMonthday day) month_Month (intYear year))
/:time/=time < (/VAR/ < (/date-entity/=entity < (/:year/=year < /NUM/=y) < (/:month/=month < /NUM/=m) < (/:day/=day < /NUM/=d)))
[relabel m /^(.+)$/$1_Month/]
[adjoinF (dayMonthYearAdv (intMonthday ss=day2) @ (intYear ss=year2)) month]
[excise month month]
[relabel y /^(.+)$/"$1"/]
[move year >1 year2]
[excise year year]
[relabel d /^(.+)$/"$1"/]
[move day >1 day2]
[excise day day]
[excise time entity]

